&lt;p class=&quot;MsoNormal&quot; style=&quot;mso-margin-top-alt: auto; text-indent: 12.0pt; mso-char-indent-count: 1.0; mso-pagination: widow-orphan;&quot;&gt;&lt;span style=&quot;font-family: 標楷體;&quot;&gt;日期&lt;span lang=&quot;EN-US&quot;&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
